&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&amp;#34;D:\Program Files\AutoHotkey\UX\AutoHotkeyUX.exe&amp;#34; &amp;#34;D:\Program Files\AutoHotkey\UX\launcher.ahk&amp;#34; &amp;#34;D:\00_zdk\myTools\AutoHotkey\sendClick-scrcpy.ahk&amp;#34; %*&lt;/span&gt;&lt;/span&gt;&lt;/code&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;
